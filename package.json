{
  "name": "electronic-mt103-hard-copy",
  "version": "1.0.0",
  "description": "Electronic MT103 Processing System",
  "scripts": {
    "cleanup": "./scripts/cleanup.sh",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && pnpm install",
    "deps:prune": "pnpm prune",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate": "tsc --noEmit"
  },
  "dependencies": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.56.0",
    "@opentelemetry/context-async-hooks": "^1.30.1",
    "@opentelemetry/core": "^1.30.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.49.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.49.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.49.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.49.1",
    "@opentelemetry/host-metrics": "^0.35.5",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/instrumentation-express": "^0.47.1",
    "@opentelemetry/instrumentation-http": "^0.47.0",
    "@opentelemetry/instrumentation-pg": "^0.47.1",
    "@opentelemetry/instrumentation-redis": "^0.46.1",
    "@opentelemetry/propagator-b3": "^1.30.1",
    "@opentelemetry/propagator-jaeger": "^1.30.1",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.47.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@sentry/node": "^7.107.0",
    "compression": "^1.7.4",
    "datadog-metrics": "^0.11.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.1",
    "generic-pool": "^3.9.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "pino": "^9.6.0",
    "piscina": "^4.3.0",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "sanitize-html": "^2.14.0",
    "socket.io": "^4.7.4",
    "winston": "^3.17.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.23",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autocannon": "^8.0.0",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@opentelemetry/api": "1.4.1"
  },
  "pnpm": {
    "overrides": {
      "@opentelemetry/api": "1.4.1"
    },
    "onlyBuiltDependencies": [
      "protobufjs"
    ]
  }
}
